<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.mtha.findmyfriends.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_list_friend"
        android:name="com.mtha.findmyfriends.ui.listfriend.ListFriendFragment"
        android:label="@string/title_friend"
        tools:layout="@layout/fragment_listfriend" />

    <fragment
        android:id="@+id/navigation_message"
        android:name="com.mtha.findmyfriends.ui.message.ChatFragment"
        android:label="@string/title_message"
        tools:layout="@layout/fragment_chat">

    </fragment>
    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.mtha.findmyfriends.ui.profile.ProfileFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/fragment_profile" />
</navigation>